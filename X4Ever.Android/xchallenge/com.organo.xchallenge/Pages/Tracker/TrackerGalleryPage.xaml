<?xml version="1.0" encoding="utf-8" ?>
<tracker:TrackerGalleryXamlPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:tracker="clr-namespace:com.organo.xchallenge.Pages.Tracker;assembly=com.organo.xchallenge"
             xmlns:controls="clr-namespace:com.organo.xchallenge.Controls;assembly=com.organo.xchallenge"
             x:Class="com.organo.xchallenge.Pages.Tracker.TrackerGalleryPage">
    <StackLayout VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
        <controls:TopBarPopupView />
        <StackLayout HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
            <ScrollView HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
                <StackLayout x:Name="stackLayout" Margin="10,0,10,0" HorizontalOptions="StartAndExpand" VerticalOptions="StartAndExpand" Orientation="Vertical"></StackLayout>
            </ScrollView>
        </StackLayout>
        <controls:FooterView />
    </StackLayout>
</tracker:TrackerGalleryXamlPage>